{
  "name": "bruno-valero-services",
  "version": "1.0.0",
  "main": "src/infra/server.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "gen-jwt-keys": "tsx ./utils/generate-async-crypto-keys.ts",
    "gen-secure-keys": "tsx ./utils/gen-secure-session-keys.ts",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest --config ./vite.config.e2e.ts",
    "test": "vitest",
    "prisma:generate": "prisma generate --schema prisma/schema-habits-tracker.prisma",
    "prisma:migrate": "prisma migrate dev --schema prisma/schema-habits-tracker.prisma",
    "migrate": "run-s prisma:migrate prisma:generate",
    "migrate:force": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "db:habits": "prisma studio --schema prisma/schema-habits-tracker.prisma",
    "dev": "tsx watch src/infra/server.ts",
    "build": "tsup src --out-dir ./build",
    "deploy": "npm i && run-s build migrate:dev gen-jwt-keys gen-secure-keys",
    "start": "node build/infra/server.js"
  },
  "keywords": [],
  "author": "Bruno Fernandes Valero",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/node": "^20.14.9",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.16.1",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/passport": "^2.5.0",
    "@fastify/secure-session": "^7.5.1",
    "@prisma/client": "^5.16.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/passport-jwt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "passport-jwt": "^4.0.1",
    "zod": "^3.23.8"
  }
}
